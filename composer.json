{
  "name": "kusflo/php-open-source-books",
  "type": "project",
  "license": "MIT",
  "description": "Proyecto para obtener libros open source",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/console": "^5.1"
  },
  "require-dev": {

  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
